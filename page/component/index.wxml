<view class="main">
    <swiper indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" circular="true">
        <block wx:for="{{imgUrls}}" wx:key="{{index}}">
            <swiper-item>
                <image src="{{item}}" class="slide-image" width="100%"/>
            </swiper-item>
        </block>
    </swiper>
    <view class="search">
        <navigator url="search/search">
            <view class="search-text">
                <icon class="weui-icon-search_in-box search-icon" type="search" size="14"></icon>
                搜 索
            </view>
        </navigator>
    </view>
    <view class="selected">
        <view class="select-title"><text>精选主题</text></view>
        <view class="select-top">
            <view class="select-top-small">
                <navigator url="list/list">
                    <image src="/image/s1.jpg"></image>
                </navigator>
            </view>
            <view class="select-top-small">
                <navigator url="list/list">
                    <image src="/image/s2.jpg"></image>
                </navigator>
            </view>
        </view>
        <view class="select-bottom">
            <navigator url="list/list">
                <image src="/image/s3.jpg"></image>
            </navigator>
        </view>
    </view>
    <view class="newest">
        <view class="newest-title"><text>新品推荐</text></view>
        <view class="newest-box">
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s4.jpg"></image>
                    <view class="newest-text"><text>低温小酸奶 罐100g</text></view>
                    <view class="newest-text"><text>￥ 5</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s5.jpg"></image>
                    <view class="newest-text"><text>原味酸奶 半斤</text></view>
                    <view class="newest-text"><text>￥ 20</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s1.jpg"></image>
                    <view class="newest-text"><text>低温小酸奶 罐 375g</text></view>
                    <view class="newest-text"><text>￥ 10.03</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s2.jpg"></image>
                    <view class="newest-text"><text>原味酸奶 半斤 100g</text></view>
                    <view class="newest-text"><text>￥ 10.01</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s4.jpg"></image>
                    <view class="newest-text"><text>纯牛奶 箱</text></view>
                    <view class="newest-text"><text>￥ 40</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s5.jpg"></image>
                    <view class="newest-text"><text>全脂纯牛奶 250ml*24盒 375g</text></view>
                    <view class="newest-text"><text>￥ 45</text></view>
                </navigator>
            </view>
            <view class="newest-list">
                <navigator url="details/details">
                    <image src="/image/s1.jpg"></image>
                    <view class="newest-text"><text>全脂高钙早餐纯牛奶250ml*24盒 箱</text></view>
